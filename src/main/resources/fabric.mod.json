{
  "schemaVersion": 1,
  "id": "ghast-farmer",
  "version": "${version}",
  "name": "Ghast Farmer",
  "description": "An addon template for the Meteor addons.",
  "authors": [
    "Logging4J"
  ],
  "contact": {
    "repo": "https://github.com/Logging4J/GhastFarmer"
  },
  "icon": "assets/template/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "dev.l4j.GhastFarmerAddon"
    ]
  },
  "mixins": [
    "addon-template.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "225,25,25"
  },
  "depends": {
    "java": ">=17",
    "minecraft": ">=${mc_version}",
    "meteor-client": "*"
  }
}
